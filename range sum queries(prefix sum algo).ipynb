    "arr = list(map(int,input().split()))\n",
    "n = len(arr)\n",
    "q = int(input())\n",
    "pre = [0]*(n+1)\n",
    "for i in range(1,n+1):\n",
    "    pre[i] = pre[i-1]+arr[i-1]\n",
    "for i in range(q):\n",
    "    l,r = map(int,input().split())\n",
    "    print(pre[r]-pre[l-1])"
